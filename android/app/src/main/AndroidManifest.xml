<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lishidatiapp">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        android:largeHeap="true"
        android:hardwareAccelerated="false"

        >
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".activity.QuizActivity"></activity>
        <activity android:name=".activity.DatiJiluActivity"></activity>
        <activity android:name=".activity.ResultActivity"></activity>
        <activity android:name=".activity.TuPuActivity"></activity>
        <activity android:name=".activity.SetAccountActivity"></activity>
        <activity android:name=".activity.AboutUsActivity"></activity>
    </application>

</manifest>